#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules/html-tokenize/bin/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules/html-tokenize/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules/html-tokenize/bin/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules/html-tokenize/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/html-tokenize@2.0.1/node_modules:/Users/terrence.keane/Code/mcp-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-tokenize/bin/cmd.js" "$@"
else
  exec node  "$basedir/../html-tokenize/bin/cmd.js" "$@"
fi
